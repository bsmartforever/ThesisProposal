@article{bland-hawthorn2013fossilimprint,
	title = {Fossil {Imprint} of a {Powerful} {Flare} at the {Galactic} {Center} along the {Magellanic} {Stream}},
	volume = {778},
	issn = {0004-637X},
	url = {http://adsabs.harvard.edu/abs/2013ApJ...778...58B},
	doi = {10.1088/0004-637X/778/1/58},
	abstract = {The Fermi satellite discovery of the gamma-ray emitting bubbles 
extending 50{\textdegree} (10 kpc) from the Galactic center has revitalized
earlier claims that our Galaxy has undergone an explosive episode in the
recent past. We now explore a new constraint on such activity. The
Magellanic Stream is a clumpy gaseous structure free of stars trailing
behind the Magellanic Clouds, passing over the south Galactic pole (SGP)
at a distance of at least 50-100 kpc from the Galactic center. Several
groups have detected faint H$\alpha$ emission along the Magellanic Stream
(1.1 {\textpm} 0.3 {\texttimes} 10-18 erg
cm-2 s-1 arcsec-2)
which is a factor of five too bright to have been produced by the
Galactic stellar population. The brightest emission is confined to a
cone with half angle $\theta$1/2 ? 25{\textdegree} roughly
centered on the SGP. Time-dependent models of Stream clouds exposed to a
flare in ionizing photon flux show that the ionized gas must recombine
and cool for a time interval To = 0.6 - 2.9 Myr for the
emitted H$\alpha$ surface brightness to drop to the observed level. A
nuclear starburst is ruled out by the low star formation rates across
the inner Galaxy, and the non-existence of starburst ionization cones in
external galaxies extending more than a few kiloparsecs. Sgr
Asstarf is a more likely candidate because it is two orders
of magnitude more efficient at converting gas to UV radiation. The
central black hole (M {\textbullet} ? 4 {\texttimes} 106 M
?) can supply the required ionizing luminosity with a
fraction of the Eddington accretion rate (fE {\textasciitilde} 0.03-0.3,
depending on uncertain factors, e.g., Stream distance) typical of
Seyfert galaxies. In support of nuclear activity, the H$\alpha$ emission
along the Stream has a polar angle dependence peaking close to the SGP.
Moreover, it is now generally accepted that the Stream over the SGP must
be farther than the Magellanic Clouds. At the lower halo gas densities,
shocks become too ineffective and are unlikely to give rise to a polar
angle dependence in the H$\alpha$ emission. Thus it is plausible that the
Stream H$\alpha$ emission arose from a "Seyfert flare" that was active
1-3 Myr ago, consistent with the cosmic ray lifetime in the Fermi
bubbles. Sgr Asstarf activity today is greatly suppressed
(70-80 dB) relative to the Seyfert outburst. The rapid change over a
huge dynamic range in ionizing luminosity argues for a compact UV source
with an extremely efficient (presumably magneto-hydrodynamic) "drip
line" onto the accretion disk.},
	urldate = {2016-03-28},
	journal = {The Astrophysical Journal},
	author = {Bland-Hawthorn, J. and Maloney, Philip R. and Sutherland, Ralph S. and Madsen, G. J.},
	month = nov,
	year = {2013},
	keywords = {accretion, accretion disks, galaxies: jets, galaxies: Seyfert, Galaxy: center, Galaxy: nucleus, Local Group},
	pages = {58}
}

@article{kalberla2005theleidenargentinebonn,
	title = {The {Leiden}/{Argentine}/{Bonn} ({LAB}) {Survey} of {Galactic} {HI}. {Final} data release of the combined {LDS} and {IAR} surveys with improved stray-radiation corrections},
