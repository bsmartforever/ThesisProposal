@article{lecoarer1993halphasurvey,
	title = {H-alpha survey of the {Small} {Magellanic} {Cloud}},
	volume = {280},
	issn = {0004-6361},
	url = {http://adsabs.harvard.edu/abs/1993A%26A...280..365L},
	abstract = {We present the first results of a kinematical H-alpha survey of the Small Magellanic Cloud (SMC). We discuss the specialized instrumentation used in this survey (telescope, focal reducer, scanning Fabry-Perot interferometers and photon counting system). We discuss also the main aspects of the data reduction. We compare the quality of our data with those obtained by other authors. We present images of the SMC showing both the intensity and the radial velocity of the H II regions and a catalog of velocities and H-alpha fluxes of 143 H II regions in this galaxy. We find that the H II region distribution follows that of the H I and we verify the existence of a gradient in velocities along the bar.},
